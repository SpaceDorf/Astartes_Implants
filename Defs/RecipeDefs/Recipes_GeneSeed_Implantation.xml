<RecipeDefs>


<RecipeDef Abstract="True" Name="SurgeryFlesh">
		<effectWorking>Surgery</effectWorking>
		<soundWorking>Recipe_Surgery</soundWorking>
		<workSpeedStat>MedicalOperationSpeed</workSpeedStat>
		<workSkill>Medicine</workSkill>
		<workSkillLearnFactor>16</workSkillLearnFactor>
</RecipeDef>


<RecipeDef  Abstract="True" Name="ImplantGeneSeedPart" ParentName="SurgeryFlesh">    
  <surgeonSurgerySuccessChanceExponent>0.5</surgeonSurgerySuccessChanceExponent>          
  <roomSurgerySuccessChanceFactorExponent>0.5</roomSurgerySuccessChanceFactorExponent>
  <surgerySuccessChanceFactor>0.95</surgerySuccessChanceFactor>
  <deathOnFailedSurgeryChance>0.01</deathOnFailedSurgeryChance>
  <workAmount>2000</workAmount>
  <recipeUsers>
    <li>Human</li>
  </recipeUsers>
  <skillRequirements>
    <li>
      <skill>Medicine</skill>
      <minLevel>1</minLevel>
    </li>
  </skillRequirements>
</RecipeDef>





<!-- 1. Second Heart -->

<RecipeDef ParentName="ImplantGeneSeedPart">
		<defName>InstallSecondHeart</defName>
		<label>Install Astartes Second Heart</label>
		<description>Installs the Second Heart in your Neophyte.</description>
		<workerClass>Recipe_InstallArtificialBodyPart</workerClass>
		<jobString>Installing Second Heart</jobString>
		<workAmount>2000</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>SecondHeart</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>SecondHeart</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>SecondHeartPlaceHolder</li>

		</appliedOnFixedBodyParts>
		<addsHediff>SecondHeart</addsHediff>
		<recipeUsers>
			<li>Human</li>

		</recipeUsers>
	</RecipeDef>

	
	
