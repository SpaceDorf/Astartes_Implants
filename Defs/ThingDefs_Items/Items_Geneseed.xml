<?xml version="1.0" encoding="utf-8" ?>

<Resources> 

  
  <!-- Definition of the GeneSeed -->

  <ThingDef ParentName="ResourceBase">
    <defName>GeneSeedZyg</defName>
    <label>Geneseed Zygote</label>
      <description>The Geneseed of your Chapter. It dates back to the Primarch of your Parent Chapter</description>
    <graphicData>
     <texPath>Things/Item/Resource/geneseed</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Standard_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>50</stackLimit>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <MarketValue>0</MarketValue>
      <Flammability>1.0</Flammability>
	  <DeteriorationRate>0</DeteriorationRate>
    </statBases>
<!--    <intricate>true</intricate> -->
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
	
	
	
  </ThingDef>
  

</Resources>

