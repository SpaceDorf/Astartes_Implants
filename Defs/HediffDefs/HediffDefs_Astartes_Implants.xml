<Defs>
  
  	<HediffDef Name="addedAstartesImplant" Abstract="True">
		<hediffClass>Hediff_AddedPart</hediffClass>
		<naturallyHealed>true</naturallyHealed>
         <defaultLabelColor>(35,150,35)</defaultLabelColor>
		<isBad>false</isBad>
	</HediffDef>


	<!-- Added body parts -->

	<HediffDef ParentName="addedAstartesImplant">
		<defName>LymansEar</defName>
		<label>lyman's ear</label>
		<spawnThingOnRemoved>LymansEar</spawnThingOnRemoved>
		<addedPartProps>
			<partEfficiency>1.0</partEfficiency>
			<isSolid>true</isSolid>
		</addedPartProps>
		<stages>
			<li>
				<capMods>
					<li>
						<capacity>Hearing</capacity>
						<offset>0.25</offset>
					</li>
				</capMods>
			</li>	 
		</stages>
	</HediffDef>

	
	
	
   
